@using TGV.IPEFAE.Web.Resources.Error;
@model System.Web.Mvc.HandleErrorInfo
@{ Layout = "~/Views/Shared/_LayoutLogado.cshtml"; }

<link href="@Url.Content("~/Content/Error.css?v=" + TGV.IPEFAE.Web.App.Controllers.BaseController.VersionNumber)" rel="stylesheet" type="text/css">
<script>
    $(function () { $('.ulDeslogado').hide(); setTimeout(function () { window.location.href = window.history.go(-1) }, 3000); $('.navbar-custom.logado').removeClass('logado'); });
</script>
<section id="Error" class="error under-construction">
    <div class="mensagem">
        <h2>@EmConstrucao.Titulo</h2>
        <h3>@EmConstrucao.AvisoPrincipal</h3>
        <h3>@EmConstrucao.AvisoSecundario</h3>
    </div>
</section>
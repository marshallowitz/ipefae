@using TGV.IPEFAE.Web.Resources.Home;
@{ Layout = "~/Views/Shared/_LayoutDeslogado.cshtml"; }

<link href="@Url.Content("~/Content/HomePage.css?v=" + TGV.IPEFAE.Web.App.Controllers.BaseController.VersionNumber)" rel="stylesheet" type="text/css">
<script src="@Url.Content("~/Scripts/Views/HomePage.js?v=" + TGV.IPEFAE.Web.App.Controllers.BaseController.VersionNumber)"></script>

<script>
    $(function () { iniciarHomePage(); });
</script>

<section id="Header" class="intro header">
    <div class="boxHome">
        <div class="box col-lg-1 col-md-1 col-sm-1 col-xs-12 text-center"></div>
        <div class="box col-lg-4 col-md-4 col-sm-4 col-xs-12 text-center">
        @*<div class="box col-lg-4 col-md-4 col-sm-6 col-xs-12 text-center">*@
            <a class="decor3_2A" href="@Url.Content("~/Estagio")">
                <p class="unPinIntro estagios">
                    <span class="title">
                        <span class="autor">@Index.ItemEstagios</span>
                    </span>
                </p>
            </a>
        </div>
        <div class="box col-lg-2 col-md-2 col-sm-2 col-xs-12 text-center"></div>
        <div class="box col-lg-4 col-md-4 col-sm-4 col-xs-12 text-center">
        @*<div class="box col-lg-4 col-md-4 col-sm-6 col-xs-12 text-center">*@
            <a class="decor3_2A" href="@Url.Content("~/Concurso")">
                <p class="unPinIntro concursos">
                    <span class="title">
                        <span class="autor">@Index.ItemConcursos</span>
                    </span>
                </p>
            </a>
        </div>
        <div class="box col-lg-1 col-md-1 col-sm-1 col-xs-12 text-center"></div>
        <div class="row col-lg-12 col-md-12 hidden-sm hidden-xs"></div>
        <div class="box col-lg-1 col-md-1 col-sm-1 col-xs-12 text-center"></div>
        @*<div class="box col-lg-6 col-md-6 col-sm-6 col-xs-12 text-center">*@
        <div class="box col-lg-4 col-md-4 col-sm-6 col-xs-12 text-center">
            <a class="decor3_2A" href="@Url.Content("~/Concurso")">
                <p class="unPinIntro processoSeletivo">
                    <span class="title">
                        <span class="autor">@Index.ItemProcessoSeletivo</span>
                    </span>
                </p>
            </a>
        </div>
        <div class="box col-lg-2 col-md-2 col-sm-2 col-xs-12 text-center"></div>
        @*<div class="box col-lg-2 col-md-2 hidden-sm hidden-xs text-center"></div>*@
        @*<div class="box col-lg-4 col-md-4 hidden col-sm-6 col-xs-12 text-center">*@
        @*<div class="box col-lg-4 col-md-4 col-sm-6 col-xs-12 text-center">
            <a class="decor3_2A" href="@Url.Content("~/Concurso")">
                <p class="unPinIntro vestibulares">
                    <span class="title">
                        <span class="autor">@Index.ItemVestibularUniFae</span>
                    </span>
                </p>
            </a>
        </div>*@
        @*<div class="box col-lg-6 col-md-6 col-sm-6 col-xs-12 text-center">*@
        <div class="box col-lg-4 col-md-4 col-sm-6 col-xs-12 text-center">
            <a class="decor3_2A" href="#services">
                <p class="unPinIntro outrosServicos">
                    <span class="title">
                        <span class="autor">@Index.ItemOutrosServicos</span>
                    </span>
                </p>
            </a>
        </div>
        @*<div class="box col-lg-2 col-md-2 hidden hidden-sm hidden-xs text-center"></div>*@
        <div class="box col-lg-1 col-md-1 col-sm-1 col-xs-12 text-center"></div>
    </div>
    <div class="scrollHome" style="clear: both;">
        <div class="row">
            <div class="col-md-12">
                <a href="#about" class="btn btn-circle page-scroll">
                    <i class="fa fa-angle-double-down animated"></i>
                </a>
            </div>
        </div>
    </div>
</section>
<section id="services">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <h2 class="section-heading">@Index.ServicesH2Servicos</h2>
                <h3 class="section-subheading text-muted">@Index.ServicesH3Principal</h3>
                <h4 class="section-subheading text-muted">@Index.ServicesH4Secundario</h4>
                <h3 class="section-subheading text-muted" style="margin-bottom: 30px; margin-top: 5px;">@Index.ServicesH3Terciario</h3>
            </div>
        </div>
        <div class="row text-center">
            <div class="col-md-4">
                <span class="fa-stack fa-4x">
                    <i class="fa fa-circle fa-stack-2x text-primary"></i>
                    <i class="fa fa-graduation-cap fa-stack-1x fa-inverse"></i>
                </span>
                <h4 class="service-heading">@Index.ServicesH4Estagio</h4>
                <p class="text-muted">@Index.ServicesPEstagio</p>
            </div>
            <div class="col-md-4">
                <span class="fa-stack fa-4x">
                    <i class="fa fa-circle fa-stack-2x text-primary"></i>
                    <i class="fa fa-suitcase fa-stack-1x fa-inverse"></i>
                </span>
                <h4 class="service-heading">@Index.ServicesH4ConcursoPublico</h4>
                <p class="text-muted">@Index.ServicesPConcursoPublico</p>
            </div>
            <div class="col-md-4">
                <span class="fa-stack fa-4x">
                    <i class="fa fa-circle fa-stack-2x text-primary"></i>
                    <i class="fa fa-group fa-stack-1x fa-inverse"></i>
                </span>
                <h4 class="service-heading">@Index.ServicesH4SocioEducativo</h4>
                <p class="text-muted">@Index.ServicesPSocioEducativo</p>
            </div>
        </div>
        <div class="row text-center">
            <div class="col-md-4">
                <span class="fa-stack fa-4x">
                    <i class="fa fa-circle fa-stack-2x text-primary"></i>
                    <i class="fa fa-comment fa-stack-1x fa-inverse"></i>
                </span>
                <h4 class="service-heading">@Index.ServicesH4Palestras</h4>
                <p class="text-muted">@Index.ServicesPPalestras</p>
            </div>
            <div class="col-md-4">
                <span class="fa-stack fa-4x">
                    <i class="fa fa-circle fa-stack-2x text-primary"></i>
                    <i class="fa fa-laptop fa-stack-1x fa-inverse"></i>
                </span>
                <h4 class="service-heading">@Index.ServicesH4Consultoria</h4>
                <p class="text-muted">@Index.ServicesPConsultoria</p>
            </div>
            <div class="col-md-4">
                <span class="fa-stack fa-4x">
                    <i class="fa fa-circle fa-stack-2x text-primary"></i>
                    <i class="fa fa-university fa-stack-1x fa-inverse"></i>
                </span>
                <h4 class="service-heading">@Index.ServicesH4Treinamentos</h4>
                <p class="text-muted">@Index.ServicesPTreinamentos</p>
            </div>
        </div>
    </div>
</section>
<section id="contato">
    @Html.Partial("_Contato")
</section>